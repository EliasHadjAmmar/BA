	journal = {Journal of Economic Growth},
	month = {December},
	number = {4},
	pages = {485-526},
	title = {{Historical social contracts and their legacy: a disaggregated analysis of the medieval republics}},
	url = {https://ideas.repec.org/a/kap/jecgro/v27y2022i4d10.1007_s10887-022-09209-1.html},
	volume = {27},
	year = 2022,
