	journal = {Journal of the European Economic Association},
	number = {4},
	pages = {561-598},
	title = {THE ECONOMIC EFFECTS OF THE PROTESTANT REFORMATION: TESTING THE WEBER HYPOTHESIS IN THE GERMAN LANDS},
