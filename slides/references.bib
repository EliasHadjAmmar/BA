	number = {2},
	pages = {339-350},
	title = {Economic integration agreements and the margins of international trade},
	url = {https://www.sciencedirect.com/science/article/pii/S0022199614000506},
	volume = {93},
	year = {2014},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/S0022199614000506},
